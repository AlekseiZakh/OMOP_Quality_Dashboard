bash# Copy this file to .env and fill in your database credentials
# Environment variables for OMOP Quality Dashboard

# Database Configuration
OMOP_DB_TYPE=postgresql
OMOP_DB_HOST=localhost
OMOP_DB_PORT=5432
OMOP_DB_NAME=omop_cdm
OMOP_DB_USER=omop_user
OMOP_DB_PASSWORD=your_password_here

# Dashboard Configuration  
DASHBOARD_DEBUG=false
DASHBOARD_HOST=localhost
DASHBOARD_PORT=8501

# Security (for future features)
SECRET_KEY=your_secret_key_here
SESSION_TIMEOUT=3600

# Logging
LOG_LEVEL=INFO
LOG_FILE=omop_dashboard.log
